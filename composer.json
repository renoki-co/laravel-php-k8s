{
    "name": "renoki-co/laravel-php-k8s",
    "description": "Just a simple port of renoki-co/php-k8s for easier access in Laravel.",
    "keywords": [
        "laravel",
        "php",
        "laravel",
        "kubernetes",
        "k8s",
        "k3s",
        "kube"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-php-k8s",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "renoki-co/php-k8s": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelK8s\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelK8s\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.28|^7.0",
        "orchestra/testbench-core": "^6.28|^7.0",
        "phpunit/phpunit": "^9.5.14"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelK8s\\LaravelK8sServiceProvider"
            ]
        },
        "laravel-zero": {
            "providers": [
                "RenokiCo\\LaravelK8s\\LaravelK8sServiceProvider"
            ]
        }
    }
}
